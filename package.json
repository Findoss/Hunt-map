{
  "name": "hunt-map",
  "version": "2.0.9",
  "homepage": "http://hunt-map.online",
  "private": true,
  "author": "Findoss",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "start": "cross-env NODE_ENV=production webpack --mode production",
    "cache": "node ./scripts/updateCache.js LD & node ./scripts/updateCache.js SB",
    "static": "node ./scripts/staticServer.js",
    "prebuild": "npm run cache",
    "nbuild": "cross-env NODE_ENV=production webpack --mode production",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "predeploy": "npm version patch && npm run build",
    "deploy": "sh ./scripts/deploy.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "firebase": "6.0.2",
    "firebaseui": "4.0.0",
    "leaflet": "1.5.1",
    "leaflet-draw": "^1.0.4",
    "leaflet-responsive-popup": "^0.6.4",
    "static-server": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "firebase-admin": "^8.2.0",
    "html-loader": "^0.5.5",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-inject-plugin": "^1.5.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
